{
  "compilerOptions": {
    "module": "NodeNext",
    "target": "es2022",
    "moduleResolution": "NodeNext",
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "composite": true,
    "stripInternal": true,
    "types": [
      "node",
      "jest"
    ],
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strictNullChecks": true,
    "inlineSourceMap": true,
    "verbatimModuleSyntax": false,
    "pretty": true
  }
}

{
  "name": "@nornir/rest",
  "version": "0.0.0",
  "description": "Tiny but mighty REST framework built on nornir",
  "author": "John Conley",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "private": false,
  "scripts": {
    "compile": "tsc -b",
    "prepare": "ts-patch i -s"
  },
  "dependencies": {
    "@nrfcloud/ts-json-schema-transformer": "^0.2.1",
    "ajv": "^8.12.0",
    "openapi-types": "^12.1.0",
    "trouter": "^3.2.1",
    "ts-json-schema-generator": "^1.2.0",
    "ts-morph": "^18.0.0",
    "tsutils": "^3.21.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.4"
  },
  "devDependencies": {
    "@nornir/core": "workspace:*",
    "ts-patch": "^3.0.0-beta3",
    "typescript": ">=5.0.4"
  },
  "exports": {
    ".": {
      "types": "./dist/runtime/index.d.mts",
      "import": "./dist/runtime/index.mjs"
    },
    "./transform": {
      "require": "./dist/transform/transform.js"
    }
  },
  "eslintConfig": {
    "rules": {
      "@typescript-eslint/no-namespace": "off"
    }
  }
}

{
  "compilerOptions": {
    "module": "NodeNext",
    "target": "es2022",
    "strict": true,
    "outDir": "dist/",
    "declaration": true,
    "skipLibCheck": true,
    "noUnusedParameters": false,
    "sourceMap": true,
    "noUnusedLocals": false,
    "rootDir": "src",
    "composite": true,
    "types": ["node"],
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}

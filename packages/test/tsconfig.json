{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "plugins": [
      {
        "transform": "@nrfcloud/ts-json-schema-transformer/dist/transform"
      },
      {
        "transform": "@nornir/rest/transform"
      }
    ],
    "tsBuildInfoFile": "dist/.tsbuildinfo",
    "outDir": "dist",
    "rootDir": "src"
  },
  "include": [
    "src/**/*",
    "src/**/*.json"
  ]
}

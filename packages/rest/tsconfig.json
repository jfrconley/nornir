{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "plugins": [
      {
        "transform": "@nrfcloud/ts-json-schema-transformer/dist/transform"
      }
    ],
    "tsBuildInfoFile": "dist/.tsbuildinfo",
    "outDir": "dist",
    "rootDir": "src",
    "paths": {
      "*": [
        "./src/typelib/*"
      ]
    }
  },
  "include": [
    "src/**/*",
    "src/**/*.json"
  ]
}
